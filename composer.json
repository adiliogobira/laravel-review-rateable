{
    "name": "adiliogobira/laravel-review-rateable",
    "description": "Review & Rating system for Laravel",
    "keywords": ["rating", "Ratable", "laravel", "Review-Rateable", "reviewable"],
    "license": "MIT",
    "authors": [
        {
            "name": "Adilio Gobira",
            "email": "adiliogobira@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "http://github.com/sumantablog/laravel-review-rateable"
        }
    ],
    "require": {
        "php" : "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3|^7.0|^8.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Sumantablog\\ReviewRateable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sumantablog\\ReviewRateable\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sumantablog\\ReviewRateable\\ReviewRateableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:windows": "vendor\\bin\\phpunit"
    }
}
